{
  "title": "DuckLL",
  "url": "https://duckll.tw",
  "version": "1.0.1",
  "maintainers": ["DuckLL"],
  "rules": [
    {
      "description": "DuckLL CapsLock to Hyper",
      "manipulators": [
        {
          "description": "capslock = esc(click) | hyper(hold)",
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "modifiers": ["right_control", "right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "caps_lock"
            }
          ]
        },
        {
          "description": "escape = capslock switch",
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "optional": ["caps_lock"],
              "mandatory": [
                "right_command",
                "right_control",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "caps_lock",
              "modifiers": ["left_control"]
            }
          ]
        },
        {
          "description": "spacebar = enter",
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ]
        }
      ]
    },
